# File: data/campaigns/Son_Of_The_Black_Eye/scenarios/08_Silent_Forest.cfg, line: 228
# File: data/campaigns/Son_Of_The_Black_Eye/scenarios/08_Silent_Forest.cfg, line: 225
#. [message]: speaker=second_unit
msgid "Very well then..."
msgstr "Muito bem então..."

#
# File: data/campaigns/Son_Of_The_Black_Eye/scenarios/08_Silent_Forest.cfg, line: 234
# File: data/campaigns/Son_Of_The_Black_Eye/scenarios/08_Silent_Forest.cfg, line: 233
