msgstr ""
#
# File: Bad_Moon_Rising/scenarios2/1_02_Rescue.cfg, line: 835
#. [message]: speaker=Raenna
# File: Bad_Moon_Rising/scenarios2/1_02_Rescue.cfg, line: 835
