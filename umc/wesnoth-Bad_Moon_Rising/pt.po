msgstr ""
#
# File: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg, line: 1790
#. [message]: speaker=unit
# File: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg, line: 1790
msgid "A box of ... little widgets... I'm not sure what they are."
msgstr ""
#
# File: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg, line: 1803
# File: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg, line: 1854
#. [message]: speaker=unit
# File: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg, line: 1803
# File: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg, line: 1854
msgid "This urn is also attached to the floor... And it spins as well."
msgstr ""
#
# File: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg, line: 1916
# File: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg, line: 1964
#. [message]: speaker=unit
# File: Bad_Moon_Rising/scenarios3/2_07_Volcano.cfg, line: 1964
