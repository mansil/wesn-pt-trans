# File: Bad_Moon_Rising/scenarios2/1_06_World.cfg, line: 257
#. [message]: speaker=Lorenzon
# File: Bad_Moon_Rising/scenarios2/1_06_World.cfg, line: 257
msgid "Well, we can't stay here, but let us visit the general store."
msgstr ""
#
# File: Bad_Moon_Rising/scenarios2/1_06_World.cfg, line: 282
#. [message]: speaker=Lorenzon
# File: Bad_Moon_Rising/scenarios2/1_06_World.cfg, line: 282
