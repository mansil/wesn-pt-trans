#. [objective]: condition=win
#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:262
msgid "Defend the city"
msgstr "Defende a cidade"

#. [event]: id=Tarsos, id=Lucia, id=Marilio, id=Tadien, id=Debra
#: The_Three_Elves/scenarios/5_Party_for_vultures.cfg:280
msgid "Tarsos"
