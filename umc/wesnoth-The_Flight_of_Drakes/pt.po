#. [unit]: id=Radd
# File: The_Flight_of_Drakes/scenarios/drakes-6.cfg, line: 88
# File: The_Flight_of_Drakes/scenarios/drakes-9.cfg, line: 212
msgid "Lieutenant Radd"
msgstr ""
