msgid "elynia_glamour+female^Rabbit"
msgstr ""


#
msgid "It may look like a mighty rabbit, but it isn’t."
msgstr ""


#
#. [attack]: type,range=pierce,melee
#. [attack]: type=pierce
msgid "incisors"
msgstr ""


#
msgid "elynia_glamour+female^Imp"
msgstr ""


#
msgid "It may look like a treacherous Imp, but it isn’t."
msgstr ""


