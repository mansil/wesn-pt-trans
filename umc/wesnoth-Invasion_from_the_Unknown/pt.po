#. [side]: type=Chaos Razerman, id=Renan
# File: Invasion_from_the_Unknown/scenarios/20_Under_the_Sands.cfg, line: 49
msgid "Renan"
msgstr ""
#
# File: Invasion_from_the_Unknown/scenarios/20_Under_the_Sands.cfg, line: 66
#. [side]: type=Verlissh Matrix Core, id=Matrix1
# File: Invasion_from_the_Unknown/scenarios/20_Under_the_Sands.cfg, line: 66
msgid "Alien Matrix"
msgstr ""
#
# File: Invasion_from_the_Unknown/scenarios/20_Under_the_Sands.cfg, line: 73
# File: Invasion_from_the_Unknown/scenarios/20_Under_the_Sands.cfg, line: 163
#. [side]: type=Shaxthal Protector Drone
