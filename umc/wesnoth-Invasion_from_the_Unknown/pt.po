msgstr ""
#
# File: Invasion_from_the_Unknown/scenarios/22A_Innuendo.cfg, line: 1503
#. [message]: speaker=Irdelkazeg
# File: Invasion_from_the_Unknown/scenarios/22A_Innuendo.cfg, line: 1503
msgid "Intruders! Kill them! They must not gain access to the lower corridor!"
msgstr ""
