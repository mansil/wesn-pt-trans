# File: Valley_of_the_Ancients/scenarios/VOTA_01_Uneasy_Alliance.cfg, line: 398
#. [modify_side]
# File: Valley_of_the_Ancients/scenarios/VOTA_01_Uneasy_Alliance.cfg, line: 406
msgid "Covard"
msgstr "Feigling"
